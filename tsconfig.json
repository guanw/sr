{
    "compilerOptions": {
        "target": "es5",
        "lib": ["es2015", "dom"],
        "module": "esnext",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "src/**/*"
    ]
}
